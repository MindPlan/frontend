{
    "compilerOptions": {
        "jsx": "react-jsx",
        "baseUrl": ".",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "target": "ES2020",
        "allowSyntheticDefaultImports": true,
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "types": [
            "vite-plugin-svgr/client"
        ],
        "include": [
            "src"
        ],
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~modules/*": [
                "src/modules/*"
            ],
            "~utils/*": [
                "src/shared/utils/*"
            ],
            "~services/*": [
                "src/shared/services/*"
            ],
            "~auth/*": [
                "src/modules/auth/*"
            ],
            "~fonts/*": [
                "src/assets/fonts/*"
            ],
            "~tag/*": [
                "src/modules/tag/*"
            ],
            "~types/*": [
                "src/types/*"
            ],
            "~routes/*": [
                "src/routes/*"
            ]
        }
    }
}