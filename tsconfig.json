{
    "compilerOptions": {
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~modules/*": [
                "src/modules/*"
            ],
            "~utils/*": [
                "src/shared/utils/*"
            ],
            "~services/*": [
                "src/shared/services/*"
            ],
            "~auth/*": [
                "src/modules/auth/*"
            ]
        }
    }
}