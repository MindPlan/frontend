{
    "compilerOptions": {
        "jsx": "react-jsx",
        "baseUrl": ".",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "target": "ES2021",
        "allowSyntheticDefaultImports": true,
        "allowImportingTsExtensions": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "types": [
            "vite-plugin-svgr/client"
        ],
        "include": [
            "src"
        ],
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~shared/*": [
                "src/shared/*"
            ],
            "~store/*": [
                "src/store/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~modules/*": [
                "src/modules/*"
            ],
            "~types/*": [
                "src/types/*"
            ],
            "~routes/*": [
                "src/routes/*"
            ],
            "~fonts/*": [
                "src/assets/fonts/*"
            ],
            "~icons/*": [
                "src/assets/icons/*"
            ],
            "~png/*": [
                "src/assets/png/*"
            ],
            "~svg/*": [
                "src/assets/svg/*"
            ],
            "~animation-images/*": [
                "src/assets/animation-images/*"
            ],
            "~calendar/*": [
                "src/modules/calendar/*"
            ],
            "~auth/*": [
                "src/modules/auth/*"
            ],
            "~dashboard/*": [
                "src/modules/dashboard/*"
            ],
            "~goals/*": [
                "src/modules/goals/*"
            ],
            "~groups/*": [
                "src/modules/groups/*"
            ],
            "~not-found/*": [
                "src/modules/not-found/*"
            ],
            "~projects/*": [
                "src/modules/projects/*"
            ],
            "~inbox/*": [
                "src/modules/inbox/*"
            ],
            "~settings/*": [
                "src/modules/settings/*"
            ],
            "~tasks/*": [
                "src/modules/tasks/*"
            ],
            "~landing/*": [
                "src/modules/landing/*"
            ],
            "~tag/*": [
                "src/modules/tag/*"
            ],
            "~components/*": [
                "src/shared/components/*"
            ],
            "~services/*": [
                "src/shared/services/*"
            ],
            "~dashboard/*": [
                "src/modules/dashboard/*"
            ],
            "~hooks/*": [
                "src/shared/hooks/*"
            ],
            "~dev/*": [
                "src/shared/dev/*"
            ],
            "~utils/*": [
                "src/shared/utils/*"
            ],
            "~styles/*": [
                "src/shared/styles/*"
            ]
        }
    }
}